<%= flash[:notice] %>
<h1>listing dojos</h1>
<table style="width:100%">
  <tr>
    <th>Coding Dojo Branch</th>
    <th>Street</th> 
    <th>City</th>
    <th>State</th>
  </tr>
  	<% @dojos.each do |dojo| %>
	  	<tr>
		    <td> <%= dojo.branch %> </td>
		    <td> <%= dojo.street %> </td> 
		    <td> <%= dojo.city %> </td>
		    <td> <%= dojo.state %> </td>
	  	</tr>
	<% end %>
</table>
<a href="/dojos/new">New Dojo</a>